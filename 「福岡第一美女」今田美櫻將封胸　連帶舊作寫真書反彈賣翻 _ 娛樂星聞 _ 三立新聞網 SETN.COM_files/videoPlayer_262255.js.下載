function drawLoading(id, prop, maxcd, cur, speed) {
	var r    = parseInt(id.getAttribute('r')),
			c    = Math.PI*(r*2),
			val  = Math.ceil(cur*100);
	(function(){
		var fc = maxcd*100;
		var pct = c * (1 - (val/fc));
		id.style[prop] = pct;
	})();
};
